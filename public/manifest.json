{
  "name": "Tec Horarios",
  "short_name": "Horarios",
  "description": "Gestión de horarios y red de amigos del Instituto Tecnológico de Ciudad Madero",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#000000",
  "theme_color": "#00529b",
  "icons": [
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "file_handlers": [
    {
      "action": "/importar-archivo",
      "accept": {
        "application/json": [".json"],
        "application/x-fdo": [".fdo"]
      },
      "icons": [
        {
          "src": "/icons/icon-512x512.png",
          "sizes": "512x512",
          "type": "image/png"
        }
      ],
      "launch_type": "single-client"
    }
  ]
}
